{
  "expo": {
    "name": "english grammar quiz",
    "slug": "english-quiz",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/images/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ajay020.englishgrammarquiz"
    },
    "web": {
      "favicon": "./app/assets/images/favicon.png"
    },
    "owner": "ajay020",
    "plugins": [
      "expo-asset"
    ]
  }
}
